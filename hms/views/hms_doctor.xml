<odoo>
    <data>
        <record  id ="doctor_list_actions" model="ir.actions.act_window">
            <field name="name">Doctors</field>
            <field name="res_model">hms.doctor</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record model="ir.ui.view" id="doctor_tree_view">
            <field name="name">doctor.tree</field>
            <field name="model">hms.doctor</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="first_name"/>
                    <field name="last_name"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="doctor_form_view">
            <field name="name">doctor.form</field>
            <field name="model">hms.doctor</field>
            <field name="arch" type="xml">
                <form string="Doctor Form">
                    <sheet>
                        <group>
                            <field name="first_name" attrs="{'required':True}"/>
                            <field name="last_name" attrs="{'required':True}"/>
                            <field name="image"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <menuitem id="doctors_menu" name="doctors" parent="hms_menu" groups="hms.manager_group"
                  action="doctor_list_actions" />

    </data>
</odoo>